  "keywords": [ "canvas", "2d", "toolkit", "draw", "geometry", "animate" ],
  "author": "Sergey Borovikov",
  "version": "0.5.3",
  "module": "./lib/bratik.es.js",
  "main": "./lib/bratik.cjs.js",
  "types": "./lib/bratik.d.ts",
  "files": [ "/lib" ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foretoo/bratik.git"
  },
  "bugs": {
    "url": "https://github.com/foretoo/bratik/issues"
  },
  "homepage": "https://github.com/foretoo/bratik#readme",
  "scripts": {
